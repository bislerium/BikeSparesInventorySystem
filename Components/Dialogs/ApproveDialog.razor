@inject ISnackbar Snackbar
@inject BikeSparesInventorySystem.Services.AuthService AuthService

<MudDialog>
    <DialogContent>
        <MudText>@ActivityLog.Quantity @Spare.Name is going to be taken out from the inventory. Are you sure to approve?</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Error" Variant="Variant.Filled" OnClick="Disapprove">Disapprove</MudButton>
        <MudButton Color="Color.Primary" Variant="Variant.Filled" OnClick="Approve">Approve</MudButton>
    </DialogActions>
</MudDialog>